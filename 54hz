getgenv().autoBuy13 = false

local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/liebertsx/Tora-Library/main/src/librarynew", true))()

local tab = library:CreateWindow("GaG/Flandre Hub")
local folder = tab:AddFolder("Blood Moon")

folder:AddLabel({
	text = "Mua Blood Event",
	type = "label"
})

folder:AddToggle({
	text = "Spam Mua Crate",
	flag = "toggle12",
	callback = function(v)
		getgenv().autoBuy12 = v
	end
})

folder:AddToggle({
	text = "Spam Mua Night Egg",
	flag = "toggle7",
	callback = function(v)
		getgenv().autoBuy7 = v
	end
})

folder:AddToggle({
	text = "Spam Mua Night Seed",
	flag = "toggle8",
	callback = function(v)
		getgenv().autoBuy8 = v
	end
})

folder:AddToggle({
	text = "Spam Mua Banana",
	flag = "toggle1",
	callback = function(v)
		getgenv().autoBuy1 = v
	end
})

folder:AddToggle({
	text = "Spam Mua Melon",
	flag = "toggle2",
	callback = function(v)
		getgenv().autoBuy2 = v
	end
})

folder:AddToggle({
	text = "Spam Mua Star Caller",
	flag = "toggle9",
	callback = function(v)
		getgenv().autoBuy9 = v
	end
})

folder:AddToggle({
	text = "Spam Mua Blood Owl",
	flag = "toggle13",
	callback = function(v)
		getgenv().autoBuy13 = v
	end
})

