-- Vòng lặp auto mua
spawn(function()
	while true do
		pcall(function()
			if getgenv().autoBuy1 then
				game:GetService("ReplicatedStorage"):WaitForChild("GameEvents"):WaitForChild("BuyEventShopStock"):FireServer("Blood Banana")
			end
			if getgenv().autoBuy2 then
				game:GetService("ReplicatedStorage"):WaitForChild("GameEvents"):WaitForChild("BuyEventShopStock"):FireServer("Moon Melon")
			end
			if getgenv().autoBuy3 then
				game:GetService("ReplicatedStorage"):WaitForChild("GameEvents"):WaitForChild("BuyEventShopStock"):FireServer("Chocolate Carrot")
			end
			if getgenv().autoBuy4 then
				game:GetService("ReplicatedStorage"):WaitForChild("GameEvents"):WaitForChild("BuyEventShopStock"):FireServer("Red Lollipop")
			end
			if getgenv().autoBuy5 then
				game:GetService("ReplicatedStorage"):WaitForChild("GameEvents"):WaitForChild("BuyEventShopStock"):FireServer("Chocolate Sprinkler")
			end
			if getgenv().autoBuy6 then
				game:GetService("ReplicatedStorage"):WaitForChild("GameEvents"):WaitForChild("BuyEventShopStock"):FireServer("Candy Blossom")
			end
			if getgenv().autoBuy7 then
				game:GetService("ReplicatedStorage"):WaitForChild("GameEvents"):WaitForChild("BuyEventShopStock"):FireServer("Night Egg")
			end
			if getgenv().autoBuy8 then
				game:GetService("ReplicatedStorage"):WaitForChild("GameEvents"):WaitForChild("BuyEventShopStock"):FireServer("Night Seed Pack")
			end
			if getgenv().autoBuy9 then
				game:GetService("ReplicatedStorage"):WaitForChild("GameEvents"):WaitForChild("BuyEventShopStock"):FireServer("Star Caller")
			end
			if getgenv().autoBuy10 then
				game:GetService("ReplicatedStorage"):WaitForChild("GameEvents"):WaitForChild("BuyEventShopStock"):FireServer("Easter Egg")
			end
			if getgenv().autoBuy11 then
				game:GetService("ReplicatedStorage"):WaitForChild("GameEvents"):WaitForChild("BuyEventShopStock"):FireServer("Candy Sunflower")
			end
			if getgenv().autoBuy12 then
				game:GetService("ReplicatedStorage"):WaitForChild("GameEvents"):WaitForChild("BuyEventShopStock"):FireServer("Mysterious Crate")
			end
			if getgenv().autoBuy13 then
				game:GetService("ReplicatedStorage"):WaitForChild("GameEvents"):WaitForChild("BuyEventShopStock"):FireServer("Blood Owl")
			end
		end)
		wait(0)
	end
end)

library:Init()
